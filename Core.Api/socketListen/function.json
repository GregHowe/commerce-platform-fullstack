{
	"bindings": [
		{
			"authLevel": "anonymous",
			"type": "httpTrigger",
			"direction": "in",
			"name": "req"
		},
		{
			"type": "http",
			"direction": "out",
			"name": "res"
		},
		{
			"type": "webPubSubConnection",
			"name": "connection",
			"userId": "{query.userid}",
			"hub": "CORE",
			"direction": "in"
		},
		{
			"type": "webPubSub",
			"name": "actions",
			"hub": "CORE",
			"direction": "out"
		}
	]
}
